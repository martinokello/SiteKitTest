@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    <div class="col-md-offset-1">
    </div>
    <div class="col-md-4">
        <b>BMI Category</b>
    </div>
    <div class="col-md-4">
        <b>Patient Count</b>
    </div>
</div>
@foreach(var row in ViewBag.BmiCategoryPatientCount)
{
<div class="row">
    <div class="col-md-offset-1">

    </div>
    <div class="col-md-4">
        @row.BmiCategory
    </div>
    <div class="col-md-4">
        @row.PatientCount
    </div>
</div>
}
